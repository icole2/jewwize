{
  "manifest_version": 3,
  "name": "Ultimate Bypass Suite",
  "version": "1.0",
  "description": "Comprehensive bypass for monitoring, filtering, and restrictions",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "proxy",
    "storage",
    "webRequest",
    "webRequestAuthProvider",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js", "injected.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon128.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "blocking_rules",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
